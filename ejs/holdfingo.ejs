                                                                                         <a value="WIN" tabindex="0" role="menuitem" class="dropdown-item" href="/WIN-INR">WIN</a>
                                                                                         <a value="BTT" tabindex="0" role="menuitem" class="dropdown-item" href="/BTT-INR">BTT</a>
                                                                                         <a value="WRX" tabindex="0" role="menuitem" class="dropdown-item" href="/WRX-INR">WRX</a>
                                                                                         </div>
                                                                                         </div>
                                                                                         <div class="btn-group">
                                                                                         </div>
                                                                                         </div>
                                                                                         <div class="right-header col-12 col-sm-12 col-md-5 col-lg-4">
                                                                                             <div class="d-inline-flex flex-wrap align-items-center justify-content-center">
              <a class="color-white" href="/connect/telegram">
              <div class="header-telegram-button">
              <div class="d-inline-block">
              <img src="/images/telegram.png" class="telegram-logo" /></div>
              <div class="text-nowrap d-inline-block color-white">Connect Telegram</div>
              </div>
              </a>
              <div class="margin-10 d-inline-block">
                  <label class="switch"><input type="checkbox" checked="" />
                      <span class="slider round"></span>
                      </label>
                      </div>
                      </div>
                      </div>
                      </div>
                      </div>
                      <div class="padding-bottom-50">
                          <div class="Container-fluid " style="padding: 0px 30px;">
                              
                                            <div class="fiat-crypto-table table-responsive" style="margin: 0px auto;">
                                                <table class="table table-borderless text-center">
                                                    <thead>
                                                        <tr>
                                                            <th>
                                                                <h4><span class="pointer">#</span></h4>
                                                                </th>
                                                                <th>
                                                                    <h4><span class="pointer">name</span></h4>
                                                                    </th>
                                                                    <th>
                                                                        <h4><span class="pointer">last</span></h4>
                                                                        </th>
                                                                        <th><h4><span class="pointer">Buy / Sell Price</span></h4>
                                                                        </th>
                                                                        <th><h4><span class="pointer">volume</span></h4>
                                                                        </th>
                                <th>
                                    <h4><span class="pointer">base_unit</span></h4>
                                </th>
                            </tr>
                        </thead>
                        <tbody>
                            <% data.forEach(function(wazir,index) { %>
                            <tr><td class="align-middle"><h4 class="table-text"><%= index + 1%></h4></td>
                                <td class="align-middle"><a target="_blank" href="https://bit.ly/2BJxccc">
                                    <h4 class="table-text">
                                        <img src="/images/bitbns.png" class="exchange-logo" />
                                        <span class="exchange-name "><%= wazir.name %></span>
                                    </h4></a></td>
                                    <td class="align-middle"><h4 class="table-text"><%= wazir.last %></h4></td>
                                    <td class="align-middle"><h4 class="table-text"><span><%= wazir.buy
                                        %> / <%= wazir.sell %></span></h4></td>
                                    <td class="align-middle"><h4 class="table-text color-green"><%= wazir.volume %></h4></td>
                                    <td class="align-middle"><h4 class="table-text color-green"><%= wazir.base_unit_of %></h4></td>
                                        </tr>
                                        <% }) %>
                                      
                                            
                                                </tr>
                                                </tbody>
                                                </table>
                                                </div>
